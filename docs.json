{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Intra Documentation",
  "colors": {
    "primary": "#4F46E5",
    "light": "#6366F1",
    "dark": "#4338CA"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Core Technology",
            "pages": [
              "core-technology/a2a-protocol",
              "core-technology/ap2-protocol"
            ]
          },
          {
            "group": "Platform",
            "pages": [
              "platform/features",
              "platform/architecture"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "use-cases",
              "pricing",
              "roadmap"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/create",
              "api-reference/endpoint/get",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://tryintra.app",
          "icon": "grid"
        },
        {
          "anchor": "Community",
          "href": "https://community.tryintra.app",
          "icon": "users"
        },
        {
          "anchor": "API Status",
          "href": "https://status.tryintra.app",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://tryintra.app"
  },
  "navbar": {
    "links": [
      {
        "label": "Blog",
        "href": "https://tryintra.app/blog"
      },
      {
        "label": "Support",
        "href": "mailto:support@tryintra.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://tryintra.app/signup"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tryintra",
      "github": "https://github.com/intra-app",
      "linkedin": "https://linkedin.com/company/intra"
    }
  },
  "analytics": {
    "plausible": {
      "domain": "docs.tryintra.app"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "search": {
    "prompt": "Search Intra documentation..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  }
}